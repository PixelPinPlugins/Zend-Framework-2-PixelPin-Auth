{
    "name": "pixelpinplugins/pixelpin-auth",
    "description": "Uses the HybridAuth PHP library to Enable authentication via PixelPin!, etc for the ZfcUserPixelpin ZF2 module.",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "pixelpin"
    ],
    "homepage": "https://github.com/PixelPinPlugins/PixelPin-Auth",
    "authors": [
        {
            "name": "Nicholas Calugar",
            "email": "njcalugar@gmail.com",
            "homepage": "http://socalnick.github.com/"
        },
        {
            "name": "Callum Brankin",
            "email": "callum@pixelpin.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-authentication": "2.*",
        "zendframework/zend-db": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-i18n": "2.*",
        "zendframework/zend-loader": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-serializer": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-session": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-test": "2.*",
        "zendframework/zend-uri": "2.*",
        "zendframework/zend-view": "2.*",
        "PixelPinPlugins/PixelPin-ZFC-User": "1.*",
        "PixelPinPlugins/PixelPin-ZFC-Base": "1.*",
        "PixelPinPlugins/PixelPin-HybridAuth": "1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.8.0"
    },
    "autoload": {
        "psr-0": {
            "PixelpinAuth": "src/",
            "PixelpinAuthTest": "test/"
        },
        "classmap": [
            "./"
        ]
    }
}
